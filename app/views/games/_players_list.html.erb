<% @game.bookings.each do |booking| %>
  <div class="player-cell">
    <% if booking.user.photo.model[:photo] == nil %>
      <%= image_tag "https://collaborativecbt.com/wp-content/uploads/2016/12/default-avatar.png", width: 100, height: 100, class: "avatar-small" %>
    <% else %>
      <%= cl_image_tag current_user.photo, width: 100, height: 100, crop: :thumb, gravity: :face, class: "avatar-small"%>
    <% end %>
    <p><%= booking.user.first_name %> <%= booking.user.last_name %></p>
    <p><%= booking.user.position %></p>
  </div>
<% end %>
